{
	"name": "@poidet/eslint-config-nuxt",
	"type": "module",
	"version": "0.8.1",
	"description": "Конфигурация ESLint для Nuxt",
	"author": {
		"url": "https://taknepoidet.xyz",
		"email": "TakNePoidet@yandex.ru",
		"name": "Yakin Nikita"
	},
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"keywords": [
		"eslint",
		"eslint-config"
	],
	"files": [
		"index.cjs"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/poidet/config.git",
		"directory": "packages/eslint-config-nuxt"
	},
	"scripts": {
		"build": "unbuild",
		"dev": "npx @eslint/config-inspector --config eslint.config.ts",
		"release": "changelogen --release",
		"typecheck": "tsc --noEmit",
		"prepublish": "pnpm run build",
		"prepare": "pnpm run build"
	},
	"dependencies": {
		"defu": "^6.1.4",
		"@nuxt/eslint-config": "^0.3.13",
		"@poidet/eslint-config": "workspace:*",
		"eslint-flat-config-utils": "^0.2.5",
		"eslint-plugin-import-x": "^0.5.1"
	},
	"devDependencies": {
		"unbuild": "^2.0.0"
	},
	"peerDependencies": {
		"eslint": ">=8.40.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
